programa {
  funcao inicio() {
    escreva("Olá, vou informar se você está aprovado, reprovado ou de recuperação.")
    
    real media
    escreva("Qual foi sua média? \n")
    leia(media)
    se (media >= 7)
    escreva("Você está aprovado!") 
    senao{
      escreva("Você está reprovado!")
    }
      }
}
